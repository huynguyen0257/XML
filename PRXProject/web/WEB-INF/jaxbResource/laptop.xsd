<?xml version="1.0" encoding="UTF-8" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://huyng/schema/laptop"
            targetNamespace="http://huyng/schema/laptop"
            elementFormDefault="qualified">
    <xsd:element name="laptop" type="Laptop"/>
    <xsd:complexType name="Laptop">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:int" minOccurs="0"/>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="price" type="xsd:positiveInteger"/>
            <xsd:element name="model" type="xsd:string"/>
            <xsd:element name="cpu" type="xsd:string"/>
            <xsd:element name="ram" type="xsd:string"/>
            <xsd:element name="lcd" type="xsd:string"/>
            <xsd:element name="weight" type="xsd:double"/>
            <xsd:element name="image" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>